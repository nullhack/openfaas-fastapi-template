[flake8]
ignore = E203, E266, E501, W503, T499
max-line-length = 79
enable-extensions=G
#select = B,C,E,F,W,T4,T0,N8,DC,DAR
import-order-style = google
docstring-convention = google
per-file-ignores = 
  tests/*:S101
max-complexity = 10
exclude =
    docs/
    .git
    __pycache__
    old
    build
    dist
